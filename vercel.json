{
    "version": 2,
    "builds": [
      { "src": "server.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/api/inventory/test-blob", "dest": "server.js" },
      { "src": "/api/inventory/(.*)", "dest": "server.js" },
      { "src": "/api/(.*)", "dest": "server.js" },
      { "src": "/(.*)", "dest": "server.js" }
    ]
  }